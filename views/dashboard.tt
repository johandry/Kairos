<!-- CONTENT -->
<div class="content">

  <!-- HEADER with title and last hour stats -->
  <div class="header">
    <div class="stats">
      <p class="stat"><span class="number">[% jobStats.lastHourStats.failure %]</span>Failed in the Last Hour</p>
      <p class="stat"><span class="number">[% jobStats.lastHourStats.running %]</span>Runing</p>
      <p class="stat"><span class="number">[% jobStats.lastHourStats.starting %]</span>Starting</p>
    </div>
    <h1 class="page-title">Dashboard</h1>
  </div>
  <!-- END of HEADER -->

  <!-- BREADCRUMB -->
  <ul class="breadcrumb">
    <li><a href="index.html">Home</a></li>
    <li class="active">Dashboard</li>
  </ul>
  <!-- END of BREADCRUMB -->

