      </div>
      <!-- END of QUICK STATS ROW -->      

      <!-- TODAY STATS ROW -->
      <div class="row">

        <!-- TODAY STATS Block -->      
        <div class="block">
          <!-- HEADER -->
          <div class="block-heading">
            <!-- 
            <span class="block-icon pull-right">     

              <a href="#" class="demo-cancel-click" rel="tooltip" title="Click to refresh"><i class="glyphicon glyphfa fa-refresh"></i></a>

            </span> 
            --> 
            <a href="#widgetlateststats" data-toggle="collapse">Today Stats</a>
          </div>

          <!-- CONTENT -->
          <div id="widgetlateststats" class="block-body collapse in">
            <table class="table">
              <thead>
                <tr>
                  <th>#</th>
                  <th>Job Name</th>
                  <th>Group</th>
                  <th>Start On</th>
                  <th>Status</th>
                  <th>End On</th>
                  <th>Exit Code</th>
                </tr>
              </thead>
              <tbody>
                [% FOR jobinstance IN jobStats.jobinstances.slice( pager.first - 1, pager.last - 1) %]
                <tr>
                  <td><a href="jobinstance/[% jobinstance.id %]/status">[% jobinstance.id %]</a>
                  </td>
                  <td><a href="jobs/[% jobinstance.job_id %]">[% jobinstance.job %]</a>
                  </td>
                  <td>[% jobinstance.group %]</td>
                  <td>[% jobinstance.start_on %]</td>
                  <td><span class="label label-default [% IF jobinstance.status == 'FAILURE' %]label-danger[% ELSIF jobinstance.status == 'SUCCESS' %]label-success[% ELSIF jobinstance.status == 'RUNNING' %]label-info[% ELSE %] [% END %]">[% jobinstance.status %]</span>
                  </td>
                  <td>[% jobinstance.end_on %]</td>
                  <td>[% jobinstance.exit_code %]</td>
                </tr>
                [% END %]
              </tbody>
            </table>
            [% INCLUDE _pagination.tt %]
          </div>
        </div>
        <!-- END of TODAY STATS Block -->        
      </div>
      <!-- END of TODAY STATS ROW -->

      <!-- NAGIOS IMAGES ROW -->
      <div class="row">      
        <!-- NAGIOS IMAGES -->
        <div class="block"> 
          <a href="#widgettodaygraphexec" class="block-heading" data-toggle="collapse">Last Executed Jobs Stats</a>
          <div id="widgettodaygraphexec" class="block-body collapse in">
              <img style="display: block; margin: 5px; padding: 0;" src="http://doxtg:D0xt6@fltva076.ffdc.sbc.com:8080/pnp4nagios/image?source=0&host=flph068&srv=DOXTG&view=0&start=-1day&graph_height=90&graph_width=1200" class="img-responsive" />
          </div>
        </div>

        <div class="block"> 
          <a href="#widgettodaygraphfail" class="block-heading" data-toggle="collapse">Last Failed Jobs Stats</a>
          <div id="widgettodaygraphfail" class="block-body collapse in">
              <img style="display: block; margin: 5px; padding: 0;" src="http://doxtg:D0xt6@fltva076.ffdc.sbc.com:8080/pnp4nagios/image?source=1&host=flph068&srv=DOXTG&view=0&start=-1day&graph_height=90&graph_width=1200" class="img-responsive" />
          </div>
        </div>
        <!-- END of NAGIOS IMAGES -->
      </div>
      <!-- END of NAGIOS IMAGES ROW -->

      <!-- ACCESS HISTORY ROW -->
      <div class="row">
        <!-- ACCESS HISTORY -->
        <div class="block">
          <!-- HEADER -->
          <a href="#widgetaccesshistory" class="block-heading" data-toggle="collapse">Today Access History</a>
          <!-- CONTENT -->
          <div id="widgetaccesshistory" class="block-body collapse in">
            <table class="table">
              <thead>
                <tr>
                  <th>Username</th>
                  <th>Full Name</th>
                  <th>Login Time</th>
                  <th style="width: 26px;"></th>
                </tr>
              </thead>
              <tbody>
                [% FOR access IN accessHistory %]
                <tr>
                  <td><p>[% access.username %]</p></td>
                  <td><p><a href="contacts/[% access.user_id %]">[% access.userfullname %]</a></p></td>
                  <td><p>[% access.accessTime %]</p></td>
                  <td> <a href="#"><i class="glyphicon glyphfa fa-list-alt"></i></a></td>
                </tr>
                [% END %]
              </tbody>
            </table>
            <!-- <p><a href="#">More...</a></p> -->
          </div>
          <!-- END of CONTENT -->
        </div>
        <!-- END of ACCESS HISTORY -->
      </div> 
      <!-- END of ACCESS HISTORY ROW -->

  <!-- END of CONTAINER -->

</div>
<!-- END of CONTENT -->

